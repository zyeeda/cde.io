{
  "name": "monostrip",
  "version": "0.1.0",
  "description": "Backend as a service(BaaS) platform powered by GraphQL.",
  "author": "Zyeeda Corporation",
  "license": "MIT",
  "main": "lib/index.js",
  "keywords": [
    "baas",
    "backend",
    "graphql",
    "sequelize"
  ],
  "files": [
    "src",
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zyeeda/monostrip.git"
  },
  "bugs": {
    "url": "https://github.com/zyeeda/monostrip/issues"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "prepublish": "npm run clean && npm run copy && npm run compile",
    "lint": "eslint src",
    "test": "BABEL_JEST_STAGE=0 NODE_ENV=test jest",
    "check": "npm run lint && npm run test",
    "clean": "rm -rf lib",
    "copy": "mkdir -p lib/config && cp src/config/*.json lib/config",
    "compile": "babel -d lib src"
  },
  "jest": {
    "testRunner": "<rootDir>/node_modules/jest-cli/src/testRunners/jasmine/jasmine2.js",
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "verbose": true,
    "unmockedModulePathPatterns": [
      "lodash",
      "core-js",
      "regenerator"
    ]
  },
  "dependencies": {
    "bunyan": "^1.6.0",
    "chance": "^1.0.0",
    "fs-plus": "^2.8.1",
    "inflection": "^1.8.0",
    "koa": "^1.1.2",
    "koa-bodyparser": "^2.0.1",
    "koa-bunyan-logger": "^1.3.0",
    "koa-etag": "^2.1.0",
    "koa-fresh": "0.0.3",
    "koa-i18n": "^1.2.0",
    "koa-locale": "^1.0.0",
    "koa-onerror": "^1.2.1",
    "koa-router": "^5.4.0",
    "lodash.isfunction": "^3.0.8",
    "lodash.merge": "^4.3.0",
    "lodash.values": "^4.1.0",
    "mysql": "^2.10.0",
    "nconf": "^0.8.4",
    "pretty-error": "^2.0.0",
    "sequelize": "^3.19.2",
    "serialize-javascript": "^1.1.2"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-eslint": "^5.0.0",
    "babel-jest": "^6.0.1",
    "babel-plugin-transform-runtime": "^6.5.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^4.0.0",
    "eslint-config-import": "^0.9.1",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-import": "^0.12.1",
    "eslint-plugin-react": "^3.16.1",
    "jest-cli": "^0.8.2"
  }
}
